#!/bin/bash
# FUNCTION: Minifies JavaScript
# ARGUMENTS: none
# AUTHOR: Jeremy Lanssiers
# COPYRIGHT: 2021 GNU
# REQUIRES: uglifyjs

location="example/"
input_file="scripts.js"
output_file="scripts.min.js"
log="update.log"

uglifyjs $location$input_file -o $location$output_file

echo Update - $(date) >> $location$log

exit 0
